---
layout: null
sitemap: false
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <!-- الصفحة الرئيسية -->
  <url>
    <loc>https://nasr-realestate.github.io/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>1.0</priority>
  </url>

  <!-- الصفحات الأساسية -->
  <url>
    <loc>https://nasr-realestate.github.io/properties.html</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>0.9</priority>
  </url>

  <url>
    <loc>https://nasr-realestate.github.io/requests.html</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>0.9</priority>
  </url>

  <!-- روابط العقارات الفردية -->
  {% for prop in site.properties %}
  <url>
    <loc>https://nasr-realestate.github.io{{ prop.url }}</loc>
    <lastmod>{{ prop.date | date_to_xmlschema | default: site.time | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.8</priority>
  </url>
  {% endfor %}

  <!-- روابط طلبات الشراء -->
  {% for req in site.requests %}
  <url>
    <loc>https://nasr-realestate.github.io{{ req.url }}</loc>
    <lastmod>{{ req.date | date_to_xmlschema | default: site.time | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.7</priority>
  </url>
  {% endfor %}

  <!-- الصفحات الأخرى -->
  {% for page in site.pages %}
  {% if page.url contains '.html' and page.url != '/404.html' and page.url != '/index.html' and page.url != '/properties.html' and page.url != '/requests.html' %}
  <url>
    <loc>https://nasr-realestate.github.io{{ page.url }}</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.5</priority>
  </url>
  {% endif %}
  {% endfor %}
</urlset>
