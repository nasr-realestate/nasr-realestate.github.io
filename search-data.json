---
layout: null
---
[
  {% for post in site.properties %}
  {
    "title": {{ post.title | jsonify }},
    "url": {{ post.url | relative_url | jsonify }},
    "price": {{ post.price | jsonify }},
    "category": {{ post.category | jsonify }},
    "image": "{{ site.url }}{{ site.baseurl }}/assets/img/properties/{{ post.image_file }}"
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
